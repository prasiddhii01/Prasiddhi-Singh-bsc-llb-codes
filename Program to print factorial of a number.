//Program to print factorial of a number.
//Developed by Prasiddhi       Date: 08/10/25
# include<stdio.h>
void main ()
{
    int n, factorial=1;
    printf("enter a number ");
    scanf("%d",&n);
    for(int i =1; i<n;i++)
    {
        factorial= factorial*i;
    }
     printf ("factorial of %d is %d", n,factorial);
    
}
