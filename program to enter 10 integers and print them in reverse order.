//program to enter 10 intgers and print them in reverse order.
//developed by  prasiddhi         date : 29/10/25
#include <stdio.h>
int main() {
    int arr[10];
    printf("Enter 10 integers:\n");
    for(int i = 0; i < 10; i++) {
        scanf("%d", &arr[i]);
    }

    printf("\nNumbers in reverse order:\n");
    for(int i = 9; i >= 0; i--) {
        printf("%d ", arr[i]);
    }
    return 0;
}
