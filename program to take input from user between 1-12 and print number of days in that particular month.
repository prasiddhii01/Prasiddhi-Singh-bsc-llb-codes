//program to print number of days in that month.
//developed by Prasiddhi     date:25/09/25
#include <stdio.h>
void main()
{
    int choice;
    printf("enter month number\n");
    scanf ("%d",&choice);
    switch (choice)
    {
        case 1: case 3: case 5: case 7: case 8: case 10: case 12:
        printf ("31 days\n");
        break;
        case 4: case 6: case 9: case 11:
        printf ("30 days\n");
        break;
        case 2: printf ("28 days\n");
        break;
        default:
        printf("number out of range");

    }

}
