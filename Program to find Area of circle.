//Program to find Area of circle.
//developed by Prasiddhi    date:11/09/25
#include <stdio.h>
void main()
{
    float radius, area;

    printf("Enter the radius of the circle: ");
    scanf("%f", &radius);
    
    area = 3.14 * radius * radius;
   
    printf("The area of the circle is: %f\n", area);

}
