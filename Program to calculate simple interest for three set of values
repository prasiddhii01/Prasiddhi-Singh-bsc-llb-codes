//Program to calculate simple interest for three set of values.
//developed by Prasiddhi           Date:18/09/25
#include <stdio.h>
void main()
{
    int i;
    float p,r,t,si;
    for(i=1;i<=3;i++)
    {
    printf("enter principle,rate of interest,time (for set%d)",i);
    scanf("%f%f%f",&p,&r,&t);
    si= (p*r*t)/100;
    printf ("simple interest for set %d =%f\n",i,si);
    }
    
}
