// Program to check  whether a year is leap year or not.
//Developed by Prasiddhi  Date : 11/09/2025
#include <stdio.h>
void main()
{
    int a;
    printf("Enter the year\n");
    scanf("%d",&a);
    if(a%4==0 && a%100!=0)
    {
        printf("Leap year");
    }
    else if (a%100==0 && a%400==0)
    {
        printf("Leap year");
    }
    else
    { 
        printf("Not a leap year");
    }
}
