//program to count and print factors of a number
//developed by Prasiddhi  date 8/10/25
#include<stdio.h>
void main ()
{
    int n,i;
    int count=0;
    printf("Enter a number");
    scanf("%d",&n);
    for(int i = 1; i<=n;i++)
    {
        if(n%i==0)
        {
            printf("%d",i);
            count++;
        }
    }
    printf("\ntotal factors are %d\n",count);
}
